<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <DisableImplicitNamespaceImports>true</DisableImplicitNamespaceImports>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>ByronAP</Authors>
    <Description>API for the Foxy Pool mining pools</Description>
    <Summary>API for the Foxy Pool mining pools</Summary>
    <Title>Foxy Pool Api Client</Title>
    <Copyright>Â© 2008-2021 ByronAP</Copyright>
    <PackageProjectUrl>https://github.com/ByronAP/FoxyPoolApi</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/ByronAP/FoxyPoolApi</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>foxypool,foxy,pool,api,chia,flax,chives,mining,bhd,signa,hddcoin,post,poc,rest,socketio</PackageTags>
    <Version>1.4.1</Version>
    <AssemblyVersion>1.4.1</AssemblyVersion>
    <FileVersion>1.4.1</FileVersion>
    <PackageIcon>fox-128.png</PackageIcon>
    <Version>1.4.1</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <PlatformTarget>AnyCPU</PlatformTarget>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <PlatformTarget>AnyCPU</PlatformTarget>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Assets\fox-128.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="5.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="RestSharp" Version="106.12.0" />
    <PackageReference Include="SocketIOClient" Version="3.0.0" />
  </ItemGroup>

</Project>
